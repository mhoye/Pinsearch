<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Pinterest Search</os:ShortName>
<os:Description>Simple Pinterest Search addon for Firefox.</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<os:Url type="text/html" method="GET" template="http://www.pinterest.com/search/pins/?q={searchTerms}">
</os:Url>
</SearchPlugin>
